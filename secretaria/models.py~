# -*- encoding: utf-8 -*-
from __future__ import unicode_literals

from django.db import models

class Servico(models.Model):
	#campos
	#métodos
	class Meta:
		abstract = True

class ConsultaAbs (Servico):
	#campos
	#métodos
	class Meta:
		abstract = True

class Consulta (ConsultaAbs):
	#campos
	#métodos
	pass

class ExameAbs (Servico):
	#campos
	#métodos
	class Meta:
		abstract = True

class Exame (ExameAbs):
	#campos
	#métodos
	pass
